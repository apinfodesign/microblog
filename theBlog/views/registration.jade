extends layout

block content
	h1= title
	p Welcome to #{title}

	p Register a New Account...
	form
		p Create New User Name:
		input(type='text', name='username')
		p Create New Password:
		input(type='text', name='password')
		p Enter Email for New Registration:
		input(type='text', name='email')
		button Submit


	!= message
